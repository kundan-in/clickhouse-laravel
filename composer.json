{
    "name": "kundan-in/clickhouse-laravel",
    "description": "ClickHouse database driver for Laravel - seamlessly integrate ClickHouse into your Laravel applications",
    "type": "library",
    "keywords": [
        "laravel",
        "clickhouse",
        "database",
        "analytics",
        "olap",
        "big-data"
    ],
    "homepage": "https://github.com/kundan-in/clickhouse-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Kundan",
            "homepage": "https://github.com/kundan-in"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "smi2/phpclickhouse": "^1.6"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "KundanIn\\ClickHouseLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KundanIn\\ClickHouseLaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KundanIn\\ClickHouseLaravel\\ClickHouseServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
